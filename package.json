{
  "name": "tech-talk1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "makeMigration": "knex migrate:make migration --knexfile db/knexfile.js",
    "migrate": "knex migrate:latest --knexfile db/knexfile.js",
    "seed": "knex seed:run --knexfile db/knexfile.js",
    "hack:client": "vue-cli-service serve",
    "hack:server": "nodemon server",
    "hack": " \"yarn hack:server\" \"yarn hack:client\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "knex": "^0.19.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
